{
  "name": "react-scaffold",
  "version": "2.0.0",
  "description": "webpack4 react",
  "main": "main.js",
  "sideEffects": false,
  "scripts": {
    "dev": "set NODE_ENV=development&& webpack-dev-server --config webpack.config.js",
    "build": "set NODE_ENV=production&& webpack -p --config webpack.config.js",
    "mac:dev": "export NODE_ENV=development&& webpack-dev-server --config webpack.config.js",
    "mac:build": "export NODE_ENV=production&& webpack -p --config webpack.config.js",
    "dev4": "webpack --mode development",
    "build4": "webpack --mode production"
  },
  "keywords": [
    "webpack4",
    "react"
  ],
  "author": "y",
  "license": "ISC",
  "dependencies": {
